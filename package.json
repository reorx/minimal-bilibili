    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.0",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.6.3",
    "webpack": "^5.72.1",
    "webpack-bundle-analyzer": "^4.5.0",
