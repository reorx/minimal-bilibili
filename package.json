{
  "name": "minimal-bilibili",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for building Chrome extensions with Webpack",
  "scripts": {
    "dev": "NODE_ENV=development webpack --watch",
    "build": "NODE_ENV=production webpack",
    "build-ext": "npm run clean && npm run build && bash pack.sh",
    "clean": "rm -rf build dist",
    "test": "npx jest",
    "format-style": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "author": "Reorx <novoreorx@gmail.com>",
  "license": "MIT",
  "dependencies": {
